generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Customer {
  id        Int      @id @default(autoincrement())
  name      String   @db.VarChar(60)
  cpf       String   @unique @db.VarChar(11)
  createdAt DateTime @default(now()) @db.Timestamp(6)

  @@map("customers")
}
